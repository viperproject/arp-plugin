method foo(x: Int) returns (y: Int)
ensures y == x+1

method bar()
{
    var x: Int := 4
    var y: Int
    y := foo(x)
    assert y == 5
    //:: ExpectedOutput(assert.failed:assertion.false)
    assert false
}

